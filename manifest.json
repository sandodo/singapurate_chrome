{
  "background": {
    "page": "background.html"
  },
   
  "manifest_version": 2,

  "name": "SingapuRate Website Filtering Service",
  "description": "Blocks websites non-suitable for your age, referring to singapurate.com, thus provides you a cloud based favorites & black list.",
  "version": "1.0.6",
  "default_locale": "en",

  "browser_action": {
    "default_icon": "skin/icon19.png",
    "default_popup": "popup.html"
  },

  "icons": {
    "16": "skin/logo16.png",
    "19": "skin/icon19.png",
    "32": "skin/icon32.png",
    "48": "skin/icon48.png",
    "128": "skin/icon128.png",
    "288": "skin/icon288.png"
  },
  
  "permissions": [
    "tabs", "storage", "webRequest", "http://*.singapurate.com/*", "http://singapurate.com/*"
  ]
}
